<table class="table table-bordered table-striped">
      <tr>

        <th>Date:</th>
        <th>Test Rufe</th>
        <th>Weight index </th>
        <th>Metabolism index</th>
      </tr>
      <tr>
        <th></th>
        <th>Value / Name</th>
        <th>Value / Name</th>
        <th>Value</th>
      </tr>
<% parent.personal_characteristics.each do |p| %>
  <tr>
    <td><h4><%= p.created_at.strftime("%d %b.%Y") %></td>
    <td><h4><%= p.rufe_index['value'] %></h4><h5><%= p.rufe_index['name'] %> </h5></td>
    <td><h4> <%= p.weight_index['value'] %></h4><h5><%= p.weight_index['name'] %></h5></td>
    <td><h4><%= p.activity_index %></h4></td>
  </tr>
<% end %>
</table>
<%= link_to "Edit", edit_user_registration_path(parent) , :class => 'btn-custom' %>