<table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th>Номер</th>
        <th>Название</th>
        <th>Описание</th>
        <th>Просмотр рациона</th>
        <th>Удаление рациона</th>
      </tr>
    </thead>
  <tbody>
  <% @trainings.each do |f| %>
    <tr>
      <td><h4><%= f.number %></h4></td>
      <td><h4><%= f.name %></h4></td>
      <td><h4><%= f.notice %></h4></td>
      <td><%= link_to "Просмотреть", f, class: "btn btn-info" %></td>
      <td><%= button_to "Удалить", f, method: :delete, class: "btn btn-danger" %></td>
    </tr>
  </tbody>
<% end %>
</table>
<div class="container text-center">
  <div class="pagination">
    <%= will_paginate @trainings, renderer: BootstrapPagination::Rails %>
  </div>
</div>

